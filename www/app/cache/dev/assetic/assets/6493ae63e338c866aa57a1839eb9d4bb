function fermeBandeauCookies()
{
    document.getElementById('bandeauCookies').style.display='none';
    document.getElementById('bandeauCookies').style.visibility='hidden';
}

function ouvreSavoirPlus(){
    window.open('informationsSurLesCookies.php');
}

function ouvre(page)
{
    window.open(page,'_blank');
}